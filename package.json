{
  "name": "yamada-ui",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.15.0",
  "license": "MIT",
  "author": "Hirotomo Yamada <hirotomo.yamada@avap.co.jp>",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamada-ui/yamada-ui"
  },
  "scripts": {
    "prepare": "lefthook install",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log .eslintcache coverage node_modules",
    "clean:all": "pnpm clean && pnpm rimraf .turbo *.log .eslintcache coverage storybook-static trace node_modules",
    "cli": "pnpm --prefix packages/cli",
    "react": "pnpm --prefix packages/react",
    "utils": "pnpm --prefix packages/utils",
    "www": "pnpm --prefix www",
    "next:pages": "pnpm --prefix playgrounds/next/pages",
    "next:app": "pnpm --prefix playgrounds/next/app",
    "vite": "pnpm --prefix playgrounds/vite",
    "remix": "pnpm --prefix playgrounds/remix",
    "hono": "pnpm --prefix playgrounds/hono",
    "prebuild": "pnpm cli build && pnpm gen:tokens",
    "build": "turbo run build --filter=./packages/**",
    "lint": "turbo run lint --filter=./packages/**",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "format": "turbo run format --filter=./packages/**",
    "typecheck": "turbo run typecheck --filter=./packages/**",
    "test": "turbo run test --filter=./packages/**",
    "spellcheck": "cspell \"**/*.{js,jsx,mjs,ts,tsx,mts,md,mdx}\" --quiet",
    "quality": "pnpm format && pnpm lint && pnpm typecheck && pnpm test",
    "gen:styles": "tsx scripts/generate/styles",
    "gen:tokens": "yamada-cli tokens packages/react/src/theme/index.ts --internal --out packages/react/src/core/generated-theme-tokens.types.ts && eslint packages/react/src/core/generated-theme-tokens.types.ts --fix",
    "gen:icons": "tsx scripts/generate/icons",
    "gen:props": "tsx scripts/generate/props",
    "changelog": "tsx scripts/changelog",
    "release": "changeset publish",
    "release:dev": "changeset version --snapshot dev && changeset publish --tag dev",
    "release:next": "changeset version --snapshot next && changeset publish --tag next"
  },
  "dependencies": {
    "@babel/parser": "^7.27.5",
    "@babel/traverse": "^7.27.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "@changesets/get-release-plan": "^4.0.13",
    "@changesets/types": "^6.1.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cspell/eslint-plugin": "^9.1.2",
    "@eslint/compat": "^1.3.1",
    "@koralle/vitest-axe": "^0.1.1",
    "@mdn/browser-compat-data": "^6.0.26",
    "@octokit/request-error": "^7.0.0",
    "@octokit/rest": "^22.0.0",
    "@storybook/addon-a11y": "^9.0.13",
    "@storybook/addon-docs": "^9.0.13",
    "@storybook/cli": "^9.0.13",
    "@storybook/react-vite": "^9.0.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__traverse": "^7.20.7",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.4",
    "@typescript-eslint/parser": "^8.35.0",
    "@typescript/analyze-trace": "^0.10.1",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vitest/ui": "^3.2.4",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/react": "workspace:*",
    "@yamada-ui/utils": "workspace:*",
    "axe-playwright": "^2.1.0",
    "commander": "^14.0.0",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-replace": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "9.0.13",
    "eslint-plugin-testing-library": "^7.5.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "execa": "9.3.1",
    "find-packages": "^10.0.4",
    "glob": "^11.0.3",
    "globals": "^16.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.14",
    "lucide-react": "^0.515.0",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.13",
    "tsdown": "^0.12.9",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "vitest-matchmedia-mock": "^1.0.6",
    "web-features": "^2.40.1"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    }
  }
}
